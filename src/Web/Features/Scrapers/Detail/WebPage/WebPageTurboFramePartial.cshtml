@model (long ScraperId, long ScraperWebPageId)
@{
    var webPageParameters = new {
        scraperId = Model.ScraperId,
        scraperWebPageId = Model.ScraperWebPageId
    };
}
<turbo-frame id="scraper_web_page_@Model.ScraperWebPageId"
             src="@(Url.Page("WebPage", webPageParameters) ?? Url.Page("WebPage/WebPage", webPageParameters))"
             data-scrapers--detail-target="webPageTurboFrame"
             >
</turbo-frame>
