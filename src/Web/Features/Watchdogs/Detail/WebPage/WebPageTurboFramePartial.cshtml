@model (long WatchdogId, long WatchdogWebPageId)
@{
    var webPageParameters = new {
        watchdogId = Model.WatchdogId,
        watchdogWebPageId = Model.WatchdogWebPageId
    };
}
<turbo-frame id="watchdog_web_page_@Model.WatchdogWebPageId"
             src="@(Url.Page("WebPage", webPageParameters) ?? Url.Page("WebPage/WebPage", webPageParameters))"
             data-watchdogs--detail-target="webPageTurboFrame"
             >
</turbo-frame>
