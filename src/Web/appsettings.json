{
  "DatabaseConnectionStringName" : "Database",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Rebus": "Debug",
        "Rebus.Bus": "Debug"
      }
    }
  },
  "AllowedHosts": "*",
  "Rebus": {
    "Transport": "InMemory", // Options: RabbitMq, PostgreSql, InMemory
    "DefaultNumberOfWorkers": 4,
    "MainNumberOfWorkers": 10,
    "ScrapingNumberOfWorkers": 4
  },
  "Jwt": {
    "ExpireMinutes": 15
  },
  "ReCaptcha": {
    "Version": "v3"
  },
  "RateLimiting": {
    "GlobalRequestsPerSecondPerUserPermitLimit": 40,
    "GlobalRequestsPerSecondPerUserQueueLimit": 0,

    "LogErrorRequestsPerSecondPerUserPermitLimit": 4,
    "LogErrorRequestsPerSecondPerUserQueueLimit": 0
  }
}
